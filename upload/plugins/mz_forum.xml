<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Форум | Mozilla Россия</os:ShortName>
<os:Description>Поиск по российскому форуму Mozilla</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<os:Url type="text/html" method="GET" template="http://forum.mozilla-russia.org/search.php">
  <os:Param name="action" value="search"/>
  <os:Param name="keywords" value="{searchTerms}"/>
  <os:Param name="author" value=""/>
  <os:Param name="forum" value="-1"/>
  <os:Param name="search_in" value="all"/>
  <os:Param name="sort_by" value="0"/>
  <os:Param name="sort_dir" value="DESC"/>
  <os:Param name="show_as" value="topics"/>
  <os:Param name="search" value="Отправить"/>
</os:Url>
</SearchPlugin>